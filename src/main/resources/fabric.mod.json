{
    "schemaVersion": 1,
    "id": "autoreconnect",
    "version": "${version}",
    "name": "Auto Reconnect",
    "description": "Automatically reconnects to the server after disconnection.",
    "authors": [
        "Me"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "CC0-1.0",
    "icon": "assets/autoreconnect/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.example.autoreconnect.AutoReconnectMod"
        ],
        "client": [
            "com.example.autoreconnect.AutoReconnectClient"
        ],
        "modmenu": [
            "com.example.autoreconnect.ModMenuIntegration"
        ]
    },
    "mixins": [
        "autoreconnect.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.16.5",
        "minecraft": "~1.21.1",
        "java": ">=21",
        "fabric-api": "*",
        "cloth-config": ">=15.0.140"
    },
    "suggests": {
        "modmenu": "*"
    }
}